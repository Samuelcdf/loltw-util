tr "game_buff_tooltip_KogMawCausticSpittleCharged" = "<titleLeft>腐蝕唾液</titleLeft><mainText>此目標降低物理防禦與魔法防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_KogMawIcathianDisplay" = "<titleLeft>來自伊卡西亞的驚喜</titleLeft><mainText>死亡後，寇格魔的身體產生連鎖反應，讓他於 4 秒後引爆；造成周圍敵人 <font color='#99FF99'>@f1@</font> 真實傷害。此效果每次升級時上升 25。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_KogMawIcathianSurprise" = "<titleLeft>來自艾卡希亞的驚喜</titleLeft><mainText>寇格魔就快爆炸了，將對附近的敵人造成傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
