tr "game_buff_tooltip_Absolute_Zero" = "<titleLeft>絕對零度</titleLeft><mainText>週遭的敵方單位被降低跑速與攻速。詠唱結束後還在技能區域內的敵人，會受到魔法傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AbsoluteZeroSlow" = "<titleLeft>凍傷</titleLeft><mainText>此單位的跑速以及攻速都被降低。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Abyssal_Scepter" = "<titleLeft>深淵權杖</titleLeft><mainText>此單位魔法防禦被降低。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Abyssal_Scepter_Aura" = "<titleLeft>深淵節杖靈氣</titleLeft><mainText>減少附近敵人的魔法防禦。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AdrenalineRushBuff" = "<titleLeft>腎上腺素啟動</titleLeft><mainText>此單位閃避率提升。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Aegis_of_the_Legion" = "<titleLeft>軍團聖盾</titleLeft><mainText>此單位得到了物理防禦、魔法防禦和攻擊力提升。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Aegis_of_the_Legion_Friend" = "<titleLeft>軍團聖盾靈氣</titleLeft><mainText>提升周圍友軍英雄的物理防禦、魔法防禦和攻擊力。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliMota" = "<titleLeft>刺客印記</titleLeft><mainText>阿卡莉攻擊被標記單位，觸發印記造成額外傷害</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliShadowDance" = "<titleLeft>暗影之舞</titleLeft><mainText>阿卡莉穿過陰影發動攻擊。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwilightShroud" = "<titleLeft>暮光防護</titleLeft><mainText>阿卡莉隱形中。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwilightShroudBuff" = "<titleLeft>暮光防護</titleLeft><mainText>阿卡莉的物理防禦和魔法防禦提高。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwilightShroudDebuff" = "<titleLeft>暮光防護</titleLeft><mainText>該單位跑速降低。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwinAP" = "<titleLeft>懲戒之力</titleLeft><mainText>阿卡莉的近戰攻擊造成 @f1@% 她魔法攻擊的額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwinDisciplines" = "<titleLeft>戰法雙修</titleLeft><mainText><font color='#FF9900' size='14'>懲戒之力：</font>獲得 20 魔法攻擊後，阿卡莉的攻擊造成 8% 魔法攻擊的額外傷害每 6 魔法攻擊增加 1% 效果。<br><br><font color='#FF9900' size='14'>懲戒之秘：</font>獲得 10 攻擊之後，阿卡莉獲得 8% 技能吸血能力，每 6 點額外攻擊力增加 1% 效果，不計算基礎攻擊<br><br>額外魔法傷害：<font color='#99FF99'>@f1@%</font><br>額外技能吸血：<font color='#99FF99'>@f2@%</font></mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AkaliTwinDmg" = "<titleLeft>懲戒之秘</titleLeft><mainText>阿卡莉獲得 @f1@% 技能吸血。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Alpha_Strike" = "<titleLeft>先聲奪人</titleLeft><mainText>此單位即將受到魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Alpha_Striking" = "<titleLeft>先聲奪人</titleLeft><mainText>易大師正在敵群之中衝鋒陷陣，並對它們造成魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharMaleficVisions" = "<titleLeft>煞星幻象</titleLeft><mainText>此單位每秒造成傷害。如果目標死於幻象持續期間，幻象跳躍到附近的敵方單位，並回復馬爾札哈的魔力。虛靈喜歡攻擊被煞星幻象擊中的單位。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharNetherGrasp" = "<titleLeft>冥府之握</titleLeft><mainText>該單位被暈眩，造成間歇性傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharNetherGraspSound" = "<titleLeft>冥府之握</titleLeft><mainText>瑪爾扎哈吟唱暈眩技。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharSummonVoidlingBuff" = "<titleLeft>召喚虛靈</titleLeft><mainText>施放 4 個技能後，瑪爾扎哈可以召喚一個虛靈來幫助戰鬥。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharSummonVoidlingReady" = "<titleLeft>召喚虛靈：就緒</titleLeft><mainText>瑪爾扎哈下個技能將會召喚虛空靈體。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharVoidling" = "<titleLeft>虛靈</titleLeft><mainText>瑪爾扎哈召喚出的不可控生物，持續 21 秒。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharVoidlingPhase2" = "<titleLeft>虛靈：成熟</titleLeft><mainText>虛靈獲得 50% 額外物理防禦和傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AlZaharVoidlingPhase3" = "<titleLeft>虛靈：狂暴</titleLeft><mainText>虛靈攻速提高 100%。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Arcane_Mastery" = "<titleLeft>奧術專精</titleLeft><mainText>任意時刻施放一個技能，所有其他的技能冷卻時間減少 1 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Archer's_Mark" = "<titleLeft>射手印記</titleLeft><mainText>該單位殺死敵人後獲得額外黃金。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Astral_Blessing" = "<titleLeft>星之祝福</titleLeft><mainText>增加此單位物理防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AuraofDespair" = "<titleLeft>絕望</titleLeft><mainText>附近敵人每秒收到傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_AuraofDespairDamage" = "<titleLeft>絕望</titleLeft><mainText>附近敵人每秒損失 2.5% 的生命。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Backstab" = "<titleLeft>背刺</titleLeft><mainText>此單位從背後發動攻擊造成額外 20% 傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Bag_of_Tea" = "<titleLeft>一包茶</titleLeft><mainText>該單位回復生命和魔力值。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BansheesVeil" = "<titleLeft>女妖面紗</titleLeft><mainText>為這個英雄抵擋一次敵方技能。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Battle_Fury" = "<titleLeft>戰鬥狂怒</titleLeft><mainText>泰達米爾每攻擊一個單位獲得 5 怒氣、暴擊獲得 10 怒氣、殺死一個單位獲得額外 10 怒氣。<br><br>每一點怒氣可增加 0.35% 的暴擊機率。<br><br>目前暴擊機率加成：<font color='#99FF99'>@f1@</font>%</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BattleHardened" = "<titleLeft>千錘百煉</titleLeft><mainText>波比每次攻擊一個目標，她獲得 1 點傷害和物理防禦，持續 8 秒。該效果可以疊加。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BerserkBuff" = "<titleLeft>狂戰士</titleLeft><mainText>此單位的攻速上升 50%、物理攻擊上升 30%，但是物理防禦與魔法防禦下降 30%。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BilgewaterCutlass" = "<titleLeft>比爾吉沃特之擊</titleLeft><mainText>該單位被減速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Black_Cleaver" = "<titleLeft>黑色切割者(@Stack@)</titleLeft><mainText>此單位物理防禦減少。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Black_Shield" = "<titleLeft>黑暗之盾</titleLeft><mainText>該單位對控制免疫，並會吸收魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BlackOmen" = "<titleLeft>黑暗預兆</titleLeft><mainText>這個單位的攻速被顯著地降低了。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Bleed" = "<titleLeft>流血</titleLeft><mainText>此單位正在流血，同時行蹤被暴露。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_BlessingoftheLizardElder" = "<titleLeft>蜥蜴長者之祝福</titleLeft><mainText>此單位的攻擊與直接傷害技能減緩目標跑速，並且在數秒內造成燃燒效果。如果此單位被殺，則增益效果將轉移給擊殺者。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Blind" = "<titleLeft>致盲</titleLeft><mainText>此單位已被致盲，無法命中目標。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Blinding_Dart" = "<titleLeft>致盲吹箭</titleLeft><mainText>這個單位很難看清東西，所以他的攻擊將不會命中目標。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
