tr "game_DynamicMenuText_[Attributes]_MovementSpeed" = "<titleLeft>跑速</titleLeft><mainText>增加英雄移動的速度，同一時間只有一雙鞋子的跑速生效。</mainText><postScriptTitle>當前速度： @Total@ （ @Base@ + @Bonus@ ）</postScriptTitle><postScriptLeft>增加跑速 @Effect@ 。</postScriptLeft>"
tr "game_DynamicMenuText_[Minimap]_AlertButtonTooltip" = "<titleLeft>標記地圖</titleLeft><titleRight>[G]</titleRight><subtitleLeft>點擊使用</subtitleLeft><mainText>點擊標記地圖或迷你地圖某一位置，你的隊友會在小地圖中看見這個閃亮的標記。<br><br><font color='#FDD017'>『召喚師注意！』 - 拉克絲</font></mainText>"
tr "game_DynamicMenuText_[Minimap]_DangerButtonTooltip" = "<titleLeft>撤退標記</titleLeft><titleRight>[V]</titleRight><subtitleLeft>點擊使用</subtitleLeft><mainText>在小地圖或者是畫面上點擊此鍵，來通知你的隊友避開此位置。你的隊伍將會在小地圖上面，暫時看到閃爍的標記。<br><br><font color='#FDD017'>『戰術性撤退！！』－斯溫</font></mainText>"
tr "game_DynamicMenuText_[Minimap]_OptionScreenTooltip" = "<titleLeft>螢幕選項</titleLeft><mainText>點擊打開選項功能表。</mainText>"
tr "game_DynamicMenuText_[Minimap]_OverlayMapButtonTooltip" = "<titleLeft>遊戲地圖</titleLeft><subtitleLeft>點擊查看地圖</subtitleLeft><mainText>此地圖是迷你地圖放大版，顯示移動單位和關鍵建築。</mainText>"
tr "game_DynamicMenuText_[Minimap]_ZoomInTooltip" = "<titleLeft>地圖放大</titleLeft><subtitleLeft>點擊使用</subtitleLeft><mainText>放大迷你地圖，顯示某地區細節資訊。</mainText>"
tr "game_DynamicMenuText_[Minimap]_ZoomOutTooltip" = "<titleLeft>地圖縮小</titleLeft><subtitleLeft>點擊使用</subtitleLeft><mainText>縮小迷你地圖，顯示更多地區的資訊。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_CharacterInfoTooltip" = "<titleLeft>英雄資訊</titleLeft><subtitleLeft>點擊打開功能表</subtitleLeft><mainText>允許查看英雄屬性和資訊。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_ClickToLevel" = "<postScriptTitle>點擊學習技能。</postScriptTitle>"
tr "game_DynamicMenuText_[PlayerHUD]_DominionTeleportHome" = "<titleLeft>回城</titleLeft><titleRight>[B]</titleRight><subtitleLeft><font color='#FF9900'>點擊使用</font></subtitleLeft><mainText>於 4.5 秒以後（強化回城 4 秒），傳送你的英雄回到基地。如受到任何傷害，則會取消傳送作業。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_GoldTooltip" = "<titleLeft>金錢</titleLeft><subtitleLeft>金錢</subtitleLeft><mainText>通過擊殺士兵、英雄和野怪獲得金錢。每過一段時間系統會發放少量金錢。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_InnateSpellHeal" = "<titleLeft>先天治癒</titleLeft><titleRight>[B]</titleRight><subtitleLeft><font color='#FF9900'>點擊使用</font></subtitleLeft><mainText>在發動期間，你可在 12 秒內回復 100% 的生命值。此效果起始時不顯著，但是會隨著時間增加而上升。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_PointsToSpend" = "<titleLeft>等級提升</titleLeft><subtitleLeft>你獲得了技能點數</subtitleLeft><mainText>點擊技能升級，提升技能威力。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_ShopDisabledTooltip" = "<titleLeft>道具商店</titleLeft><titleRight>[P]</titleRight><subtitleLeft><font color='#FFCC00' size='14'>太遠不能購買物品。請點右鍵移動到商店範圍</font></subtitleLeft><mainText>進入商店範圍後，商店的按鈕會發亮，同時可以點擊進入商店頁面。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_ShopInfoTooltip" = "<titleLeft>道具商店</titleLeft><titleRight>[P]</titleRight><subtitleLeft>點擊打開目錄</subtitleLeft><mainText>只能在商店範圍內購買物品。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_SummonerInfoTooltip" = "<titleLeft>召喚師資訊</titleLeft><subtitleLeft>點擊打開功能表</subtitleLeft><mainText>點擊查看召喚師屬性和資訊。 (該功能暫時停用)</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_TeleportHome" = "<titleLeft>回城</titleLeft><titleRight>[B]</titleRight><subtitleLeft><font color='#FF9900'>點擊使用</font></subtitleLeft><mainText>8 秒（強化回城 7 秒）後將英雄傳送回基地復活點。任何傷害都會中斷傳送。</mainText>"
tr "game_DynamicMenuText_[PlayerHUD]_UnlearnedSpell" = "<postScriptTitle><font color='#FF0000'>你還沒有學會這個技能！</font></postScriptTitle>"
