tr "game_buff_tooltip_UrAniumRounds" = "<titleLeft>烏’安紐姆式口徑</titleLeft><mainText>這個單位的攻擊會減少目標 1 點的物理防禦和魔法防禦。（最多可疊加到 50 ）</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrAniumRoundsHit" = "<titleLeft>烏’安紐姆式口徑-減益</titleLeft><mainText>這個單位的物理防禦和魔法防禦被減少了。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotCorrosiveDamage" = "<titleLeft>諾克薩斯酸性彈</titleLeft><mainText>此單位受到持續傷害，同時烏爾加特的酸蝕獵手將會自動鎖定攻擊此單位。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotCorrosiveDebuff" = "<titleLeft>諾克薩斯酸性彈</titleLeft><mainText>此單位減少物理防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotEntropyDebuff" = "<titleLeft>佐恩螺旋增壓器</titleLeft><mainText>該單位傷害減少 15% ，持續 2.5 秒。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotEntropyPassive" = "<titleLeft>退化螺栓</titleLeft><mainText>烏爾加特的攻擊降低敵方對其 15% 的傷害，持續 3 秒。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotSwapDef" = "<titleLeft>超動力防禦 </titleLeft><mainText>使用超時空傳送器後，烏爾加特的物理防禦和魔法防禦得到提升。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotSwapExecute" = "<titleLeft>超時空傳送器</titleLeft><mainText>該單位最近剛被超時空傳送器傳送過。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotSwapTarget" = "<titleLeft>超時空傳送器</titleLeft><mainText>該單位成為了超時空傳送器的目標，且被暈眩。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_UrgotTerrorCapacitor" = "<titleLeft>恐怖電容</titleLeft><mainText>烏爾加特獲得吸收 @f1@ 點傷害的護盾。並且他的攻擊和法術將減低敵人跑速 @f2@% 。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ValkyrieSound" = "<titleLeft>女武神特攻</titleLeft><mainText>庫奇發動俯衝突襲，留下死亡與毀滅的火焰之路。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VanguardAura" = "<titleLeft>守衛靈氣</titleLeft><mainText>鄰近英雄增加物理防禦與魔法防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VanguardBuff" = "<titleLeft>護衛</titleLeft><mainText>此單位擁有 @f1@ 強化物理防禦與魔法防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneCondemn" = "<titleLeft>審判</titleLeft><mainText>此單位被汎的巨型努箭擊退。如果碰撞到地形物的話，會產生暈眩的效果。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneInquisition" = "<titleLeft>死亡通緝令</titleLeft><mainText>汎對敵人發出死亡通緝，獲得物理攻擊強化，強化被動技能效果，另外使用翻滾瞄準時附加潛行效果。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneInquisitionStealth" = "<titleLeft>潛行</titleLeft><mainText>汎進入潛行狀態。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneSilverBolts" = "<titleLeft>神聖銀箭</titleLeft><mainText>對同個目標使用 3 次普通攻擊或技能，就會造成目標最大生命值特定百分比的真實傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneSilverDebuff" = "<titleLeft>神聖銀箭</titleLeft><mainText>汎如果攻擊目標三次，會造成額外的傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VayneTumble" = "<titleLeft>翻滾瞄準</titleLeft><mainText>汎的下次普通攻擊力上升。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VeigarEquilibrium" = "<titleLeft>均衡</titleLeft><mainText>維迦每損失 1% 的魔力，就會增加 0.75% 的魔力回復。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Visionary" = "<titleLeft>法術幻想</titleLeft><mainText>努努攻擊 7 次後下個法術不消耗魔力。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Visionary_Counter" = "<titleLeft>法術幻想</titleLeft><mainText>努努正在準備使用無消耗的技能。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Visions" = "<titleLeft>法術幻想</titleLeft><mainText>努努下個法術不消耗魔力。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VladimirBloodGorged" = "<titleLeft>血色契約</titleLeft><mainText>每 40 點生命給予弗拉迪米爾 1 點魔法攻擊，每 1 點魔法攻擊給予弗拉迪米爾 1.8 點生命（不會迴圈疊加）。<br><br>額外魔法攻擊：<font color='#99FF99'>@f1@</font><br>額外生命：<font color='#99FF99'>@f2@</font></mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VladimirHemoplagueDebuff" = "<titleLeft>血之瘟疫</titleLeft><mainText>該單位會受到額外傷害，一段時間後會引爆傷害</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VladimirSanguinePool" = "<titleLeft>血紅之池</titleLeft><mainText>弗拉迪米爾無法被選定，使血泊上的敵人減速，並吸取血泊上單位的生命，持續 2.5 秒。 </mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VladimirTidesofBloodCost" = "<titleLeft>血之潮汐： 強化</titleLeft><mainText>每疊加一層會增加弗拉迪米爾的治療和回復效果 8% ，每疊加一層後，下次釋放血之潮汐可以造成 25% 額外傷害（最高 100% ），增加消耗 50% （最高200% ）。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VoidStone" = "<titleLeft>虛空之石</titleLeft><mainText>卡薩丁受到的魔法傷害降低 15% ，並將降低的傷害轉換成額外的攻速。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VoidStoneEmpowered" = "<titleLeft>虛空之石</titleLeft><mainText>卡薩丁攻速提升 @f1@% 。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearE" = "<titleLeft>狂吼震天</titleLeft><mainText>此單位被弗力貝爾的怒吼震攝造成緩速效果。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearHatred" = "<titleLeft>仇視時光守護者</titleLeft><mainText><font color='#FDD017' >『就算是極靈那老頭也無法阻止武裝熊加入英雄聯盟。』</font></mainText>"
tr "game_buff_tooltip_VolibearHatredZilean" = "<titleLeft>仇視武裝熊</titleLeft><mainText><font color='#FDD017' >『只要有老夫活著的一天，將永遠不允許武裝熊加入英雄聯盟。』</font></mainText>"
tr "game_buff_tooltip_VolibearPassiveBuff" = "<titleLeft>暴雪的祝福</titleLeft><mainText>如果生命低於 30% 時，弗力貝爾將在 6 秒內回復最高生命值的 @f1@% 。冷卻時間 120 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearPassiveCD" = "<titleLeft>暴雪的祝福：無法使用</titleLeft><mainText>弗力貝爾近期回復了生命，必須再次儲存力量。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearPassiveHeal" = "<titleLeft>暴雪的祝福：治療中</titleLeft><mainText>弗力貝爾正在大幅度的恢復生命。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearQ" = "<titleLeft>積蓄怒火</titleLeft><mainText>弗力貝爾的下次攻擊會將目標擊飛至空中。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearQSpeed" = "<titleLeft>野性猛熊：速度</titleLeft><mainText>弗力貝爾如果追擊一名敵方英雄，可提昇跑速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearRApplicator" = "<titleLeft>轟雷電爪</titleLeft><mainText>弗力貝爾的攻擊發出連鎖閃電，並且對多名敵人造成傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearWBuff" = "<titleLeft>熊性大發：攻速</titleLeft><mainText>弗力貝爾增加攻速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_VolibearWStats" = "<titleLeft>熊性大發：攻速</titleLeft><mainText>弗力貝爾增加攻速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Voracity" = "<titleLeft>貪婪</titleLeft><mainText>獲得擊殺或助攻將給予卡特蓮娜額外 25 黃金，並減少卡特蓮娜技能冷卻 15 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Vorpal_Spikes" = "<titleLeft>恐懼尖刺</titleLeft><mainText>科加斯的攻擊會喚出穿透敵人的利刺來打擊目標背後的敵人。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wall_of_Pain" = "<titleLeft>痛苦之牆</titleLeft><mainText>該單位的物理防禦與魔法防禦減少。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wall_of_Pain_Slow" = "<titleLeft>痛苦之牆</titleLeft><mainText>該單位的跑速大大減少，但會慢慢地恢復正常。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WeaponMastery" = "<titleLeft>武器專精</titleLeft><mainText>賈克斯從裝備道具所獲得的攻擊力基礎上額外獲得 15% 攻擊加成。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wild_Card" = "<titleLeft>萬能牌</titleLeft><mainText>逆命攻擊額外產生一個亂數值的魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WillOfTheAncientsFriendly" = "<titleLeft>遠古意志</titleLeft><mainText>這個單位的技能吸血和魔法攻擊得到了提升。</mainText><postScriptLeft>來自：  @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wither" = "<titleLeft>枯萎</titleLeft><mainText>此單位攻速和跑速隨時間減少。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WitsEndBuff" = "<titleLeft>智慧末刃</titleLeft><mainText>此單位的魔法防禦增加 @f1@。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WizardsRegalia" = "<titleLeft>戰鬥法師</titleLeft><mainText>此單位物理防禦和魔法攻擊提高。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WormAttack" = "<titleLeft>貪欲侵蝕</titleLeft><mainText>這個單位的攻擊力被嚴重降低了。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wrath_of_the_Ancients" = "<titleLeft>遠古之怒</titleLeft><mainText>此單位受到更高的魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_WriggleLanternWard" = "<titleLeft>瑞格之燈</titleLeft><mainText>該守衛正在照亮周圍區域。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Wuju_Style" = "<titleLeft>無極劍道</titleLeft><mainText>易大師物理攻擊增加。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoBattleCry" = "<titleLeft>仰天長嘯</titleLeft><mainText>趙信增加了攻速，並且每攻擊一次，冷卻時間隨之減少。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoBattleCryHeal" = "<titleLeft>阿凡達技能</titleLeft><mainText>趙信增加了生命以及攻速。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoBattleCryPassive" = "<titleLeft>仰天長嘯</titleLeft><mainText>趙信增加了攻速。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoComboAuto" = "<titleLeft>三重爪擊</titleLeft><mainText>趙信的下一次攻擊造成額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoComboAutoFinish" = "<titleLeft>三重爪擊</titleLeft><mainText>趙信的下一次攻擊對士兵造成額外的傷害，並且將他的攻擊目標打飛到空中。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoComboTarget" = "<titleLeft>三重爪擊</titleLeft><mainText>趙信的下一次攻擊造成額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoKnockup" = "<titleLeft>三重爪擊</titleLeft><mainText>該單位被趙信打飛到空中。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoParry" = "<titleLeft>橫掃千軍</titleLeft><mainText>趙信增加了魔法防禦以及物理防禦。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoPuncture" = "<titleLeft>不倦勇者</titleLeft><mainText>趙信每 3 次攻擊可以回復 <font color='#99FF99'>@f1@</font> 生命。此效果每升 2 級可提高 5。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XenZhaoSweepArmor" = "<titleLeft>衝鋒戰甲</titleLeft><mainText>趙信增強了物理防禦。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XerathAscended" = "<titleLeft>魔導封印</titleLeft><mainText>齊勒斯吞噬秘法能量，讓他獲得抵抗物理傷害的能力。他將 @f2@% 的魔法攻擊轉換為物理防禦。<br><br>物理防禦加成：<font color='#99FF99'>@f1@</font></mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XerathBarrage" = "<titleLeft>秘法彈幕</titleLeft><mainText>齊勒斯可以使用秘法彈幕數次。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XerathBolt" = "<titleLeft>幻魔鎖鏈：標記</titleLeft><mainText>齊勒斯的下次魔法攻擊，會造成目標暈眩狀態。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_XerathLocusOfPower" = "<titleLeft>魔導軌跡</titleLeft><mainText>增加技能攻擊範圍與魔法穿透強化。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickDecayingGhoul" = "<titleLeft>瘟疫</titleLeft><mainText>一陣充滿疾病的灰雲壟罩著此食屍鬼，造成週遭敵方緩速效果。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickOmenPreDeath" = "<titleLeft>死亡之兆</titleLeft><mainText>約瑞科製造出一名友軍的複製靈魂。如果該英雄死亡時，靈魂還生存在場上，則會復活英雄一段時間。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickOmenReanimated" = "<titleLeft>死亡</titleLeft><mainText>此單位被約瑞科復活，並且尋求死亡的復仇。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickOmenRevenant" = "<titleLeft>複製靈魂</titleLeft><mainText>約瑞科製造出一名友軍的複製靈魂。如果該英雄死亡時，靈魂還生存在場上，則靈魂會犧牲自己復活英雄一段時間。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickRavenousGhoul" = "<titleLeft>飢荒</titleLeft><mainText>此食屍鬼所造成的傷害將治癒約瑞科。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickSpectralBuffSelf" = "<titleLeft>迅捷的速度</titleLeft><mainText>當迅捷食屍鬼在場上時，約瑞科的跑速更快。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickSpectralGhoul" = "<titleLeft>戰爭</titleLeft><mainText>此食屍鬼增加了跑速，並且每次攻擊造成額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickSpectralPreHit" = "<titleLeft>戰爭之兆</titleLeft><mainText>約瑞科的下次攻擊造成額外傷害，並召喚迅捷食屍鬼...</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_YorickUnholySymbiosis" = "<titleLeft>邪惡契約</titleLeft><mainText>每召喚一隻食屍鬼，約瑞科減少 5% 傷害，同時一般攻擊增加 5%。此外，每隻食屍鬼擁有約瑞科 35% 的攻擊力與生命值。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Zephyr" = "<titleLeft>和風守護</titleLeft><mainText>珍娜的跑速提升，並可以穿越單位。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
