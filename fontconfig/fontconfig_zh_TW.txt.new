tr "game_buff_tooltip_KogMawBioArcaneBarrage" = "<titleLeft>生化彈幕</titleLeft><mainText>寇格魔的射程得到了提升，並且他的攻擊能造成目標最大生命一定百分比的魔法傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
