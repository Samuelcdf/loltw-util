tr "game_buff_tooltip_Scout's_Bounty" = "<titleLeft>斥候的禮物</titleLeft><mainText>此單位正被提摩追蹤，物理防禦減少，顯示在敵人的視野內，並給提摩帶來額外獎勵。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Scurvy" = "<titleLeft>烈酒利刃</titleLeft><mainText>剛普朗克的基本攻擊與槍火談判附帶劇毒效果，每秒造成 @f1@ 魔法傷害並緩速 7%。持續 3 秒最多疊加 3 次。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ScurvyStrike" = "<titleLeft>烈酒利刃</titleLeft><mainText>此單位被緩速，並持續受到傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Seal_Fate" = "<titleLeft>命運</titleLeft><mainText>此單位每秒受到傷害並無法使用技能。<br></mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Second_Sight" = "<titleLeft>灌鉛骰子</titleLeft><mainText>逆命和他的盟友在每殺死一個單位後，會獲得額外 2 枚黃金。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SeismicShard" = "<titleLeft>地震碎片</titleLeft><mainText>墨菲特偷取目標的跑速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShadowStepDodge" = "<titleLeft>暗影防護</titleLeft><mainText>卡特蓮娜收到的傷害減低。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShadowWalk" = "<titleLeft>潛行</titleLeft><mainText>除了鄰近英雄以外，此單位將無法被發現行蹤。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shatter" = "<titleLeft>寶石碎片</titleLeft><mainText>塔里克擊碎寶石讓碎片削弱你的物理防禦幾秒鐘。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShatterAura" = "<titleLeft>碎片靈氣</titleLeft><mainText>塔里克的寶石使得他的隊友提昇物理防禦。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShatterSelfBonus" = "<titleLeft>寶石盔甲</titleLeft><mainText>塔里克盔甲上面的寶石能量提昇物理防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Sheen" = "<titleLeft>閃耀劍</titleLeft><mainText>此單位下次攻擊造成額外傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SheenDelay" = "<titleLeft>閃耀劍冷卻</titleLeft><mainText>閃耀劍、巫妖之禍和三相之力正在冷卻。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Destabilize" = "<titleLeft>被動搖</titleLeft><mainText>這個單位的物理防禦被降低了，並且在被攻擊時，會使攻擊方恢復生命。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Feint_Buff" = "<titleLeft>佯攻</titleLeft><mainText>慎將吸收一些傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Feint_Proc" = "<titleLeft>佯攻</titleLeft><mainText>慎的閃避率提高 @f1@%  。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Passive_Aura" = "<titleLeft>氣合斬</titleLeft><mainText>下一次攻擊會對目標造成額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Passive_Marker" = "<titleLeft>氣合斬</titleLeft><mainText>每過 8 秒鐘，慎的下一次攻擊就會造成 @f2@ 魔法傷害。慎每被敵方英雄攻擊一次，氣合斬就會減少 2 秒的冷卻時間。<br><br>來自額外生命的傷害：<font color='#99FF99'>@f3@</font><br>基本傷害： <font color='#99FF99'>@f1@</font></mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Shadow_Dash" = "<titleLeft>影襲</titleLeft><mainText>慎正在快速衝鋒，並對沿途的敵人進行嘲諷。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Shadow_Dash_Taunt" = "<titleLeft>嘲諷</titleLeft><mainText>該單位被嘲諷。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Stand_United_Channel" = "<titleLeft>並肩作戰</titleLeft><mainText>慎正在傳送，以援助友軍。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Stand_United_Shield" = "<titleLeft>並肩作戰</titleLeft><mainText>吸收 @f1@ 點傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Stand_United_Target" = "<titleLeft>並肩作戰</titleLeft><mainText>慎正在傳送，以援助友軍。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Vorpal_Star" = "<titleLeft>利刃</titleLeft><mainText>對這個單位發動攻擊，將會治療攻擊方。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shen_Vorpal_Star_Heal" = "<titleLeft>利刃</titleLeft><mainText>這個單位正在持續恢復生命。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Shield_Marker" = "<titleLeft>花崗岩護盾</titleLeft><mainText>當墨菲特 10 秒內沒有受到傷害，他會召喚一個岩石護盾以吸收相當於他最大生命 10% 的傷害。</mainText><postScriptLeft>來自: @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaDoubleAttack" = "<titleLeft>雙龍聚首</titleLeft><mainText>希瓦娜的下次攻擊將攻擊兩次。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaDoubleAttackDragon" = "<titleLeft>雙龍聚首</titleLeft><mainText>希瓦娜的下次攻擊將會對所有面前的敵人造成兩次傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaDragonScales" = "<titleLeft>御守龍甲</titleLeft><mainText>希瓦娜的物理防禦與魔法防禦獲得暫時的提昇。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaFlameBreathDebuff" = "<titleLeft>龍之吐息</titleLeft><mainText>此單位的物理防禦被降低，同時希瓦娜的攻擊將會點燃目標造成額外傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaPassive" = "<titleLeft>龍族後裔</titleLeft><mainText>希瓦娜的攻擊強化她的技能。<br><br><font color='#FF9900'>雙龍聚首</font> － 減少技能冷卻時間 0.5 秒。<br><font color='#FF9900'>聖龍怒火</font> － 延長技能持續時間 1 秒鐘，最多延長 6 秒。<br><font color='#FF9900'>龍之吐息</font> － 對弱化目標造成魔法攻擊 15% 的技能傷害。<br><font color='#FF9900'>真龍轉生</font> － 攻擊造成 2 點怒氣，人類型態時每 1.5 秒可獲得 1 點怒氣。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaScorchedEarth" = "<titleLeft>聖龍怒火</titleLeft><mainText>希瓦娜的跑速增加，周圍敵人將隨著時間而受到傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaScorchedEarthDragon" = "<titleLeft>聖龍怒火</titleLeft><mainText>希瓦娜的跑速增加，同時她於身後留下一道火焰路徑，對停留在上面的敵人造成傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_ShyvanaTransform" = "<titleLeft>聖龍型態</titleLeft><mainText>希瓦娜化身為強大的聖龍。當希瓦娜擁有怒氣時，她的技能獲得額外範圍效果，同時真龍轉生所待來的防禦加成效果加倍。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SightWard" = "<titleLeft>幻象守衛</titleLeft><mainText>這個守衛正在監視周圍的區域。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Silence" = "<titleLeft>沉默</titleLeft><mainText>此單位無法使用技能。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Silence_Attack" = "<titleLeft>沉默攻擊</titleLeft><mainText>這只蜥蜴的普通攻擊會使目標沉默。</mainText><postScriptLeft>來源於： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SilentKiller" = "<titleLeft>殺手意志</titleLeft><mainText>伊芙琳受到 50% 士兵傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SiphoningStrike" = "<titleLeft>虹吸打擊</titleLeft><mainText>納瑟斯下次攻擊造成額外傷害，如果虹吸打擊殺死了單位，則下次虹吸打擊傷害加強。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SivirPassive" = "<titleLeft>敏捷疾行</titleLeft><mainText>希維爾對敵方英雄的基本攻擊提昇 50 跑速，持續 2 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SivirPassiveSpeed" = "<titleLeft>戰鬥動能</titleLeft><mainText>此單位增加跑速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerExoskeleton" = "<titleLeft>結晶骨骼</titleLeft><mainText>史加納的骨骼化為結晶狀，讓他獲得短暫的額外護盾。當此護盾持續時，他的跑速與攻速獲得提昇。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerFracture" = "<titleLeft>水晶塵爆</titleLeft><mainText>史加納可以吸收此單位的能量來治療自己。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerImpale" = "<titleLeft>蠍尾穿刺</titleLeft><mainText>史加納已經壓制了你，並且可以拖著你移動。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerImpaleBuff" = "<titleLeft>蠍尾穿刺</titleLeft><mainText>史加納壓制一名敵人，並且拖著目標移動期間，仍然可以使用水晶斬擊與結晶骨骼。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerVirulentSlash" = "<titleLeft>水晶斬擊</titleLeft><mainText>史加納使用水晶能量強化自己，讓下次的水晶斬擊造成額外的魔法傷害並且緩速敵人。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SkarnerVirulentSlashSlow" = "<titleLeft>水晶斬擊：緩速</titleLeft><mainText>此單位被減緩跑速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
