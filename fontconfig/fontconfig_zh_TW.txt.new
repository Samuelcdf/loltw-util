tr "game_buff_tooltip_KogMawLivingArtillery" = "<titleLeft>生化巨炮</titleLeft><mainText>寇格魔的下一發生化巨炮將會根據疊加層數消耗額外的魔力（每層 40 點，最多 200 點）。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
