tr "game_buff_tooltip_SonaHymnofValorAura" = "<titleLeft>英勇讚美詩</titleLeft><mainText>索娜彈奏英勇讚美詩，增加附近友軍攻擊力和魔法攻擊。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
