tr "game_buff_tooltip_SonaSongofDiscordAuraB" = "<titleLeft>靈氣：迅捷奏鳴曲</titleLeft><mainText>此單位提升跑速。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
