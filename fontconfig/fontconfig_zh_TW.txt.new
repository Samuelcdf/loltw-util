tr "game_buff_tooltip_SniperRange" = "<titleLeft>狙擊射程</titleLeft><mainText>此單位攻擊距離極大提高。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_SonaAriaofPerseveranceAura" = "<titleLeft>堅毅詠歎調</titleLeft><mainText>索娜彈奏堅毅詠歎調，增加附近友軍英雄的物理防禦和魔法防禦。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
