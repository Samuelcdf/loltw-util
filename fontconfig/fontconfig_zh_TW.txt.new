tr "game_buff_tooltip_SoulNexus" = "<titleLeft>死神形態</titleLeft><mainText>納瑟斯的攻擊加強，此外納瑟斯從每個敵人身上每 5 秒偷取 4 ／ 5 ／ 6 生命。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
