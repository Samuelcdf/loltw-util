tr "game_buff_tooltip_RumbleOverheat" = "<titleLeft>系統過熱！</titleLeft><mainText>藍寶處於系統過熱狀態，基本攻擊獲得額外的魔法傷害，但是無法使用任何技能。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_RumbleShield" = "<titleLeft>鐵屑電磁盾</titleLeft><mainText>藍寶格檔掉攻擊。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_RumbleShieldBuff" = "<titleLeft>強力噴射器！</titleLeft><mainText>藍寶正在迅速的移動。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Rune_Prison" = "<titleLeft>符文監獄</titleLeft><mainText>這個單位不能移動或攻擊，並且每 4 秒內受到 40 點傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Rupture" = "<titleLeft>破裂</titleLeft><mainText>此單位被擊飛，不能行動。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Rupture_Target" = "<titleLeft>破裂</titleLeft><mainText>此單位跑速降低。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Rylais" = "<titleLeft>瑞萊的冰晶節杖</titleLeft><mainText>你的英雄下次攻擊會降低目標 35% 的跑速，持續 2 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_RylaisDebuff" = "<titleLeft>瑞萊的冰晶節杖</titleLeft><mainText>跑速減少 35% ，持續 2 秒。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Sadism" = "<titleLeft>背水一戰</titleLeft><mainText>蒙多醫生消耗生命，短暫提升跑速並巨幅提高生命回復速度。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
