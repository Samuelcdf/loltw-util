tr "game_buff_tooltip_MonkeyKingSpinToWin" = "<titleLeft>無極風暴</titleLeft><mainText>悟空隨著時間過去，贈加自己的跑速。旋轉時，周圍敵人會受到傷害並且擊飛。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserCOTGDot" = "<titleLeft>死亡之子</titleLeft><mainText>這個單位正在受到持續傷害，這些傷害將會被用來治療魔鬥凱撒。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserCOTGPet" = "<titleLeft>囚徒</titleLeft><mainText>這個單位正在聽從魔鬥凱撒的指使。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserCOTGSelf" = "<titleLeft>死亡之子</titleLeft><mainText>魔鬥凱薩已經控制了一個敵人的靈魂，獲得了他 1/5 的攻擊力（@f1@）和魔法攻擊（@f2@）。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserCreepingDeathBuff" = "<titleLeft>死亡蔓延</titleLeft><mainText>周圍的敵人正在持續受到傷害。另外，這個單位的物理防禦和魔法防禦得到了提高。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserIronMan" = "<titleLeft>鐵人</titleLeft><mainText>每當魔鬥凱薩用他的技能造成傷害後，這些傷害值的 @f2@% 都將轉化為一個臨時護盾。護盾最大值為 @f1@ ，每秒衰減 3% 。此技能對小兵使用時擁有一半的效果。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
tr "game_buff_tooltip_MordekaiserMaceOfSpades" = "<titleLeft>鏟擊之錘</titleLeft><mainText>魔鬥凱撒的下一次攻擊，將猛力揮出他的釘頭錘，錘的餘力將對最多 3 個額外的敵人進行打擊，造成他的物理攻擊以及額外傷害。如果只傷害到一個目標，將造成額外傷害。</mainText><postScriptLeft>來自： @SourceName@</postScriptLeft>"
tr "game_buff_tooltip_Mourning" = "<titleLeft>劊子手的標記</titleLeft><mainText>該單位承受持續傷害。</mainText><postScriptLeft>來自：@SourceName@</postScriptLeft>"
